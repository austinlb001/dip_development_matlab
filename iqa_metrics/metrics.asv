function [psnr_value,ssim_value]=metrics(img, original_img)

% 1. PSNR 
% 2. SSIM
% 3. CW-SSIM
% 4. UNIQUE 
% 5. MS-UNIQUE
% 6. CSV
% 7. SUMMER


% image resize 

if size(img,1)~= size(original_img,1)
    img = imresize(img,[size(original_img,1) size(original_img,2)],"bilinear");
end


% PSNR 

psnr_value = psnr(img,original_img);


% SSIM 

ssim_value = ssim(img, original_img);

% CW-SSIM


% MS-UNIQUE


% CSV


% SUMMER

end